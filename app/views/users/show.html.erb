<header>
  <div class="header-inner">
    <div class="toggle_btn">
      <span></span>
      <span></span>
      <span></span>
    </div>
    <div id="mask"></div>
    <div class="header-inner-title">
      <%= @nickname %>さんの投稿一覧
    </div>
    <%= link_to image_tag("kinkomilogo2.png", class:"top-page"), "/" %>
  </div>
</header>
<div class="center-contents-user-show">
  <div class="post-space-index">
    <% @posts.each do |post| %>
      <div id="post_<%= post.id %>">
        <%= render 'posts/post', post: post %>
      </div>
    <% end %>
  </div>
</div>
